{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
  "linterOptions": {
    "exclude": [
      "src/app/utilities/message-template-util.ts"
    ]
  },
  "jsRules": {},
    "rules": {
      "no-console": false,
      "quotemark": [true, "single"],
      "object-literal-sort-keys": false,
      "trailing-comma": [true, {
        "singleline": "never",
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }],
      "no-empty": [true, "allow-empty-functions"],
      "interface-name": false
    },
    "rulesDirectory": []
}